model Document {
  id           String       @id @default(uuid())
  name         String
  description  String?
  date         DateTime
  petId        String
  pet          Pet          @relation(fields: [petId], references: [id])
  attachments  Attachment[]
  createdAt    DateTime     @default(now())
  updatedAt    DateTime     @updatedAt
  attachmentId String?
}
